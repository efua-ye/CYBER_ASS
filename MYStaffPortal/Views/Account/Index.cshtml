
@model StaffPortal.Entities.ApplicationUser
@{
    ViewData["Title"] = "User Profile";
}
<!-- ============================================================== -->

<div class="row bg-title">
    <div class="col-lg-12">
        <h4 class="page-title">Profile</h4>
        <ol class="breadcrumb">
            <li><a href="#">Dashboard</a></li>
            <li class="active">Profile Page</li>
        </ol>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- row -->

    <div class="table-responsive">
        <form asp-action="Index" method="post" class="mt-4">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="UserName"> UserName</label>
                <input type="text" asp-for="UserName" class="form-control" placeholder="Enter Faculty Name" >
                <span asp-validation-for="UserName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FirstName"> FirstName</label>
                <input type="text" asp-for="FirstName" class="form-control" placeholder="Enter Faculty Name" required="">
                <span asp-validation-for="FirstName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LastName"> LastName</label>
                <input type="text" asp-for="LastName" class="form-control" placeholder="Enter Faculty Name" required="">
                <span asp-validation-for="LastName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Email"> Email</label>
                <input type="text" asp-for="Email" class="form-control" placeholder="Enter Faculty Name" required="">
                <span asp-validation-for="Email" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PhoneNumber"> PhoneNumber</label>
                <input type="text" asp-for="PhoneNumber" class="form-control" placeholder="Enter Faculty Name" required="">
                <span asp-validation-for="PhoneNumber" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="SecurityStamp"> SecurityStamp</label>
                <input type="text" asp-for="SecurityStamp" class="form-control" placeholder="Enter Faculty Name" required="">
                <span asp-validation-for="SecurityStamp" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="NormalizedEmail"> NormalizedEmail</label>
                <input type="text" asp-for="NormalizedEmail" class="form-control" placeholder="Enter Faculty Name" required="">
                <span asp-validation-for="NormalizedEmail" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NormalizedUserName"> SecurityStamp</label>
                <input type="text" asp-for="NormalizedUserName" class="form-control" placeholder="Enter Faculty Name" required="">
                <span asp-validation-for="NormalizedUserName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EmailConfirmed"> EmailConfirmed</label>
                <input type="text" asp-for="EmailConfirmed" class="form-control" placeholder="Enter Faculty Name" required="">
                <span asp-validation-for="EmailConfirmed" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PasswordHash"> PasswordHash</label>
                <input type="text" asp-for="PasswordHash" class="form-control" placeholder="Enter Faculty Name" required="">
                <span asp-validation-for="PasswordHash" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ConcurrencyStamp"> SecurityStamp</label>
                <input type="text" asp-for="ConcurrencyStamp" class="form-control" placeholder="Enter Faculty Name" required="">
                <span asp-validation-for="ConcurrencyStamp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SecurityStamp"> SecurityStamp</label>
                <input type="text" asp-for="SecurityStamp" class="form-control" placeholder="Enter Faculty Name" required="">
                <span asp-validation-for="SecurityStamp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SecurityStamp"> SecurityStamp</label>
                <input type="text" asp-for="SecurityStamp" class="form-control" placeholder="Enter Faculty Name" required="">
                <span asp-validation-for="SecurityStamp" class="text-danger"></span>
            </div>

            <button type="submit" class="btn btn-success mr-2">Submit</button>
            <a href="Cancel" type="button" class="btn btn-dark">Cancel</a>
        </form>
    </div>

    
